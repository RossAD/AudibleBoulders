{
  "name": "AudibleBoulders",
  "version": "1.0.0",
  "description": "A sublime plugin for Git Workflow",
  "main": "",
  "scripts": {
    "start": "nodemon src/server/server.js",
    "initDB": "mysql -u root -p < src/server/schema.sql",
    "openDB": "mysql -u root -p app",
    "initTestDB": "mysql -u root -p < src/server/test-schema.sql",
    "openTestDB": "mysql -u root -p test",
    "addTestData": "npm run initTestDB && mysql -u root < src/server/test-data/test-data.sql",
    "lint": "jshint src",
    "test": "karma start karma.conf.js",
    "forever" : "forever start src/server/server.js"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AudibleBoulders/AudibleBoulders.git"
  },
  "author": "Albert Huynh, Alice Yu, Diamond Wheeler, Ross Davis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AudibleBoulders/issues"
  },
  "homepage": "http://www.gitspy.com/",
  "dependencies": {
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "fs": "0.0.2",
    "morgan": "^1.6.1",
    "mysql": "^2.4.2",
    "nodemon": "^1.8.1",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.10",
    "request": "^2.69.0",
    "underscore": "^1.8.3",
    "sockets": "^0.4.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "grunt": "^0.4.4",
    "grunt-contrib-concat": "^0.3.0",
    "grunt-contrib-cssmin": "^0.9.0",
    "grunt-contrib-uglify": "^0.4.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-eslint": "^17.3.1",
    "grunt-git": "^0.3.7",
    "grunt-mocha-test": "^0.10.0",
    "grunt-nodemon": "^0.2.1",
    "grunt-shell": "^0.6.4",
    "jshint": "^2.9.1",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^0.2.2",
    "karma-nyan-reporter": "0.0.43",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.5",
    "pre-commit": "^1.1.2",
    "sinon": "^1.17.3"
  }
}
